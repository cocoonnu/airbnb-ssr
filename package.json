        "vue-client-only": "^2.1.0",
        "vue-i18n": "^9.3.0-beta.16",
        "vue-lazyload": "^3.0.0-rc.2",
        "vue-router": "^4.0.13",
        "vuex": "^4.0.2",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^18.14.0",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
